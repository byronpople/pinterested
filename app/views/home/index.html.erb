<div class="jumbotron jumbotron-fluid center">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="display-4">Welcome to My Pinterested App</h1>
                <br>
            </div>

        </div>
        <div class="row">
            <div class="col-lg-12">
                <% if user_signed_in? %>
                    <%= link_to 'Logout', destroy_user_session_path, method: :delete, class: 'btn btn-outline-dark' %>
                    <%= link_to 'List Pins', pins_path, class: 'btn btn-outline-dark' %>
                <% else %>
                    <%= link_to 'Log In', new_user_session_path, class: 'btn btn-outline-dark' %>
                    <%= link_to 'Register', new_user_registration_path, class: 'btn btn-outline-dark' %>
                <% end %>
            </div>
        </div>
    </div>
</div>
