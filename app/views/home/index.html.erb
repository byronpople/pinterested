<div class="jumbotron jumbotron-fluid center">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="display-4">Welcome to My Pinterested App</h1>
                <br>
            </div>

        </div>
        <div class="row">
            <% if user_signed_in? %>
          <div class="col-lg-12">
                <%= link_to 'Logout', destroy_user_session_path, method: :delete, class: 'btn btn-dark' %>
                <%= link_to 'List Pins', pins_path, class: 'btn btn-defualt' %>
            </div>
        <% else %>
          <div class="col-lg-12">
                <%= link_to 'Log In', new_user_session_path, class: 'btn btn-dark' %>
                <%= link_to 'Register', new_user_registration_path, class: 'btn btn-primary' %>
            </div>
        <% end %>
        </div>

    </div>
</div>
